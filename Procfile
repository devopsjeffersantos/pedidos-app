web: java -jar target/pedidos-0.0.1-SNAPSHOT.jar -Dspring.datasource.url=${DATASOURCE_URL} -Dspring.datasource.username=${DATASOURCE_USERNAME} -Dspring.datasource.password=${DATASOURCE_PASSWORD} -Daws.sqs.endpoint=${AWS_SQS_ENDPOINT_ENVIA_EMAILS} -Daws.credentials.client-id=${AWS_CLIENT_ID} -Daws.credentials.client-secret=${AWS_SECRET_ID} -DAPI_CLIENTE_ENDPOINT=${API_CLIENTE_ENDPOINT} -DAWS_SQS_ENDPOINT_PROCESSA_PEDIDOS=${AWS_SQS_ENDPOINT_PROCESSA_PEDIDOS} -DAPI_PRODUTO_ENDPOINT=${API_PRODUTO_ENDPOINT} -Daws.region=us-east-1 -Dserver.port=${PORT}
